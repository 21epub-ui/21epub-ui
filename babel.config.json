{
  "presets": [
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "entry",
        "corejs": { "version": "3.24", "proposals": true }
      }
    ],
    [
      "@babel/preset-typescript",
      {
        "allowDeclareFields": true,
        "allowNamespaces": false
      }
    ],
    [
      "@babel/preset-react",
      {
        "runtime": "automatic",
        "importSource": "@emotion/react"
      }
    ]
  ]
}
